  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/utils/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 20
      }
    },
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    },
    "globals": {
